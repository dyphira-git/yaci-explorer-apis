{
	"name": "yaci-explorer-apis",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"build": "yarn workspace @yaci/client build",
		"typecheck": "yarn workspace @yaci/client typecheck",
		"migrate": "./scripts/migrate.sh",
		"migrate:dry": "./scripts/migrate.sh --dry-run",
		"decode:evm": "npx tsx scripts/decode-evm.ts",
		"decode:evm:daemon": "npx tsx scripts/decode-evm-daemon.ts",
		"decode:evm:priority": "npx tsx scripts/decode-evm-single.ts",
		"backfill:evm": "npx tsx scripts/backfill-evm-contracts.ts",
		"backfill:evm:logs": "npx tsx scripts/backfill-evm-logs.ts",
		"test:evm-hash-lookup": "npx tsx scripts/test-evm-hash-lookup.ts"
	},
	"dependencies": {
		"@grpc/grpc-js": "^1.12.0",
		"bech32": "^2.0.0",
		"ethers": "^6.15.0",
		"pg": "^8.13.0",
		"postgres": "^3.4.8",
		"protobufjs": "^7.4.0"
	},
	"devDependencies": {
		"@types/pg": "^8.11.0",
		"tsx": "^4.19.0",
		"typescript": "^5.6.0"
	}
}
